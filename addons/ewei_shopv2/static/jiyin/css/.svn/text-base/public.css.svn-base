html,
body,
div,
span,
applet,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
a,
abbr,
acronym,
address,
big,
cite,
code,
del,
dfn,
em,
img,
ins,
kbd,
q,
s,
samp,
small,
strike,
strong,
sub,
sup,
tt,
var,
b,
u,
i,
center,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
article,
aside,
canvas,
details,
embed,
figure,
figcaption,
footer,
header,
hgroup,
menu,
nav,
output,
ruby,
section,
summary,
time,
mark,
audio,
video,
input,
textarea,
select {
	margin: 0;
	padding: 0;
	border: 0;
	font: inherit;
	vertical-align: baseline;
	outline: 0;
	box-sizing: border-box;
	background-color: transparent;
}
a,img,button,input,textarea{-webkit-tap-highlight-color:rgba(255,255,255,0);}
*::-webkit-scrollbar {display:none}
select, input, textarea {
	appearance: none;
	-moz-appearance: none;
	-webkit-appearance: none;
	resize: none;
	-webkit-tap-highlight-color:rgba(255,255,255,0)
}
ul, li {list-style-type: none;}
a,a:active,a:hover {color: inherit;text-decoration: none;}
.page {
	position: absolute;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	overflow: hidden;
	background-color: #f0f0f0;
	color: #333333;
	font-family: "microsoft yahei";
}
.over_1 {
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap
}

.over_2 {
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden
}
.loadding {
    font-size: 12px;
    text-align: center;
    color: #666;
    padding: 5px;
}
.tab {
	display: flex;
	display: -webkit-flex;
	background-color: #fff;
	line-height: 0.84rem;
	border-bottom: 1px solid #C3C3C3;
}
.tab .item {
	position: relative;
	width: 25%;
}
.tab .item.active {
	color: #ff0000;
}
.tab .item.active::after {
	content: '';
	position: absolute;
	bottom: 0;
	height: 1px;
	left: 50%;
	width: 70%;
	margin-left: -35%;
	background-color: #ff0000;
}
.block {
	display: block;
}
.hide {
	display: none
}
.img-100>img {
	display: block;
	width: 100%;
}
.content {
	position: absolute;
	top: .9rem;
	bottom: 0;
	left: 0;
	right: 0;
	-webkit-overflow-scrolling: touch;
	overflow: auto;
}
.clearfix:after {
	content: '';
	display: block;
	width: 0;
	height: 0;
	overflow: hidden;
	clear: both
}
.c-0 {
	color: #000
}

.c-f {
	color: #fff
}

.fz-40 {
	font-size: .4rem
}

.fz-30 {
	font-size: .3rem
}

.fz-32 {
	font-size: .32rem
}

.fz-24 {
	font-size: .24rem
}

.fz-26 {
	font-size: .26rem
}

.fz-34 {
	font-size: .34rem
}

.fz-28 {
	font-size: .28rem
}

.fl {
	float: left
}

.fr {
	float: right
}

.tl {
	text-align: left
}

.tc {
	text-align: center
}

.tr {
	text-align: right
}

.f-b {
	font-weight: 700
}

.f-n {
	font-weight: normal
}
.btn {
	display: block;
	line-height: .84rem;
	color: #fff;
	width: 100%;
	text-align: center;
	font-size: .3rem;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px
}
.btn:active, .btn:hover {
	color: #fff;
}
.btn input {
	display: none
}

.btn-primary {
	background-color: #46bbb9
}

.btn-primary:active {
	background-color: #46bbb9
}

.btn-normal {
	background-color: #f26620
}

.btn-mar {
	margin: .4rem 0
}

.content-pad {
	padding: 0 .2rem
}

.mask {
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	left: 0;
	background-color: rgba(0, 0, 0, 0.3);
	z-index: 11;
}
.fv, .fh, .fhv, .fs {display: flex; display: -webkit-flex;}
.fv {align-items: center; -webkit-align-items: center;}/*垂直居中*/
.fh {justify-content: center; -webkit-justify-content: center;}/*水平居中*/
.fs {justify-content: space-between; -webkit-justify-content: space-between;}
.fhv {justify-content: center; -webkit-justify-content: center;align-items: center; -webkit-align-items: center;}/* 垂直水平居中 */
.footer {position: absolute;bottom: 0;width: 100%;display: flex;display: -webkit-flex;height: 60px;text-align: center;background-color: #fff;z-index: 9;}
.footer .item {display: block;width: 100%;font-size: 13px;color: #333333;text-decoration: none;}
.footer .img img {width: 0.68rem; }
.footer .img {height: 0.65rem; margin-bottom: 0.1rem;}
.swiper-container img { display: block; width: 100%; }
.swiper-container-horizontal>.swiper-pagination {line-height: 0;bottom: 50px;}
.swiper-pagination .swiper-pagination-bullet {
	width: 8px;
	height: 8px;
	background-color: #fff;
	opacity: 1;
	border-radius: 0;
}
.swiper-pagination .swiper-pagination-bullet.swiper-pagination-bullet-active {
	background-color: #999999;
}
/*三角形 S*/
.triangle-left {
	width: 0;
	height: 0;
	border-top: .1rem solid transparent;
	border-bottom: .1rem solid transparent;
	border-right: .1rem solid #c92c73;
}

.triangle-right {
	width: 0;
	height: 0;
	border-top: .1rem solid transparent;
	border-bottom: .1rem solid transparent;
	border-left: .1rem solid #c92c73;
}

/*三角形 E*/
.sq-img img {
    display: block;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
}

.sq-img {
    position: relative;
    height: 0;
    padding-bottom: 100%;
    overflow: hidden;
}
/* 空页面样式 S */
#empty, .empty-wrap {
	height: 100%;
}
.empty-wrap {
	padding: 0.8rem 0.25rem;
	text-align: center;
	background-color: #fff;
}

.empty-wrap img {
	display: block;
	width: 2.4rem;
	margin: 0 auto;
}

.empty-wrap p {
	color: #999999;
	font-size: 0.26rem;
	margin: 0.2rem 0;
}
.empty-wrap a {display: block;font-size: 0.28rem;color: #46bbb9;}

.empty-wrap.empty-data img {
	width: 80%;
}
/* 空页面样式 E */
/* 商城选择参数 S */
.sel-bottom-wrap {
    position: fixed;
    width: 100%;
    bottom: 0;
    background-color: #fff;
    overflow: hidden;
    border-top: 1px solid #E8E8E8;
    transition: all .5s cubic-bezier(0.42, 0, 0, 0.04);
    z-index: 99;
}

.sel-bottom-wrap .top {
    line-height: 0.9rem;
    font-size: 0.3rem;
    color: #c92c73;
}
.sel-bottom-wrap .center {
    padding-bottom: 0.9rem;
    font-size: 0.24rem;
    overflow: hidden;
}

.params-list li {
    width: 50%;
    line-height: 0.5rem;
    float: left;
    margin: 0.1rem 0;
}

.params-list {
    padding: 0 0.2rem;
    overflow: hidden;
}

.params-list li img {
    width: 0.4rem;
    vertical-align: middle;
    margin-right: 0.25rem;
}

.sel-bottom-wrap .top a {
    position: relative;
    font-size: 0.6rem;
    transform: rotateZ(45deg);
    -webkit-transform: rotateZ(45deg);
    z-index: 99;
}
.sel-bottom-wrap .center .s-img {width: 100%; height: 2.7rem; margin: 0.1rem 0;}
.params-item {
    padding: 0.1rem 0;
    overflow: hidden;
    margin-left: -2%;
    margin-right: -2%;
}

.params-item dt {
    padding: 0 0 .2rem .2rem;
    font-size: 0.24rem;
}

.params-item dd {
    width: 21%;
    float: left;
    line-height: 0.7rem;
    text-align: center;
    margin: 0 2%;
    margin-bottom: 0.2rem;
    background-color: #f0f0f0;
}

.params-item dd.active {
    background: #fff url(../img/sel_params_bg.png) no-repeat;
    background-size: 100% 100%;
}

.sel-bottom-wrap .bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    line-height: 0.8rem;
    color: #fff;
    font-size: 0.3rem;
    text-align: center;
    background-color: #a61a59;
}
/* 商城选择参数 E */
/* 收货地址 S */
.address_main {
	padding: 0.2rem;
}

.address_main .line {
	/* padding: 0.3rem 0; */
	border-bottom: 1px solid #ddd;
}

.address_main .line input,
.address_main .line sele {
	width: 100%;
}

#address_container {
	display: none;
	position: absolute;
	bottom: 0;
	width: 100%;
	height: 100%;
	background-color: #fff;
	z-index: 999;
	transform: translateX(100%);
	transition: all .3s ease-in;
	font-size: 0.28rem;
}

.address_main .line {
	height: 1rem;
	line-height: 1rem;
	border-bottom: 1px solid #ddd;
}

.address_main .line input,
.address_main .line select {
	display: block;
	width: 100%;
	height: 100%;
}

.address_sub1,
.address_sub2 {
	height: 0.8rem;
	line-height: 0.8rem;
	text-align: center;
	width: 46%;
	margin: 0.2rem 2%;
	float: left;
}

.address_sub1 {
	background-color: #a61a59;
	color: #fff;
}

.address_sub2 {
	background-color: #ddd;
	color: #fff;
}

.row {
	display: flex;
	line-height: 0.8rem;
	height: 0.8rem;
	padding: 0 0.3rem;
}

.row .title {
	width: 20%;
}

.row .info {
	width: 80%;
}

.row .info input {
	width: 100%;
}
.address {
	position: relative;
	padding: 0.3rem 0.2rem;
	border-bottom: 1px solid #efefef;
}

.address .ico {
	position: absolute;
	top: 50%;
	margin-top: -6px;
	display: block;
	height: 12px;
}

.address .name {
	margin-bottom: 0.1rem;
}

.address .info {
	padding-left: 25px;
	overflow: hidden;
}

.address .edit {
	position: absolute;
	right: 0.2rem;
	top: 50%;
	margin-top: -6px;
}
/* 收货地址 E */